<!--pages/distributionMoney/distributionMoney.wxml-->
<view class='p_40 flex_cb'>
  <view class='f_28'>
    分销佣金
    <view class='f_34 mt_20'>{{money}}元</view>
  </view>
  <view class='postal f_30' data-type="1" bindtap="goCommission">提现明细</view>
</view>
<view class='p_20 flex_cb f_32 bor_t'>
  可提现佣金
  <view>{{can_money}}元</view>
</view>
<view class='p_20 flex_cb f_32 bor_top'>
  已提现佣金
  <view>{{yet_money}}元</view>
</view>
<navigator url='../distributionIllustrate/distributionIllustrate' class='p_20 flex_cb f_32 bor_top bor_b' u>
  用户须知
  <image class="arrow ml_20" src="../../images/last_Dawn.png"></image>
</navigator>
<button class='s_foot disHeader f_32' bindtap='bindtap' data-type='1'>我要提现</button>
